{
  "name": "farert-wasm",
  "version": "1.0.0",
  "description": "C/C++ WebAssembly development environment",
  "main": "dist/main.js",
  "scripts": {
    "build": "source setup_env.sh && make",
    "clean": "make clean",
    "serve": "make serve",
    "dev": "source setup_env.sh && make && make serve",
    "setup": "source setup_env.sh",
    "test": "node test_simple.js",
    "test:unit": "node tests/run_tests.js",
    "test:simple": "node test_simple.js",
    "test:database": "node tests/run_tests.js database",
    "test:route": "node tests/run_tests.js route",
    "test:fare": "node tests/run_tests.js fare",
    "test:integration": "node tests/run_tests.js integration",
    "test:browser": "echo 'Open http://localhost:8080/test_unit.html in browser after running: npm run serve'"
  },
  "keywords": [
    "webassembly",
    "wasm",
    "c",
    "cpp",
    "emscripten"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {},
  "engines": {
    "node": ">=14.0.0"
  }
}